import matplotlib.pyplot as plt
x=([20,25,35,40,43,47])
y=([1990,1995,2000,2005,2010,2015])
plt.plot(x,y , marker='o',color='r',linestyle=':')
plt.title('Car sales')
plt.ylabel('year')
plt.xlabel('numbers in thousands')
plt.show()

v=[5,6,7,8,12,10]
n=[2000,2002,2004,2006,2008,2010]
plt.scatter(v,n,marker='p')
plt.title('inflation over time')
plt.xlabel('inflation')
plt.ylabel('years')
plt.show()
